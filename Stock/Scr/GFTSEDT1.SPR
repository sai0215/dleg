FUNCTION _ras0n601f     &&  m.sdes VALID
#REGION 1
IF ISDIGIT(m.sdes)
   IF spop('protab',"number+' '+des",2,21,10,39,'sdes','number')
      SELECT protab
      m.sprotab = serial
      m.sname = des
      m.sdes  = number
      DO getuprice
      m.sprice = m.sqty*m.suprice
      isser_nbr = ser_nbr
      SET ORDER TO serial
      SELECT (alias)
      SHOW GETS
   ELSE
      SELECT protab
      RETURN 0
   ENDIF
ELSE
  IF spop('protab',"SUBSTR(des,1,21)+' '+ number",2,21,10,39,'sdes','des')
