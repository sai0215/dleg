      m.sprice = m.sqty*m.suprice
      isser_nbr = ser_nbr
      SET ORDER TO serial
      SELECT (alias)
      SHOW GETS
   ELSE
      SELECT protab
      RETURN 0
   ENDIF
ENDIF
